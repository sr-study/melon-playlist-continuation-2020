            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Evaluate",
            "type": "python",
            "request": "launch",
            "program": "evaluate.py",
            "args": [
                "evaluate",
                "--gt_fname=arena_data/answers/val.json",
                "--rec_fname=arena_data/results/results.json"
            ],
            "console": "integratedTerminal"
        }
